{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-microsoft-contrib",
        "tslint-config-standard",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "no-void-expression": false,
      "type-literal-delimiter": false,
      "no-http-string": false,
      "quotemark": [
        true,
        "single",
        "avoid-escape"
      ],
      "indent": [
        true
      ],
      "interface-name": [
        false
      ],
      "no-duplicate-case": false,
      "no-duplicate-switch-case": true,
      "no-stateless-class": false,
      "no-unnecessary-class": true,
      "no-var-self": false,
      "no-this-assignment": [true, {"allowed-names": ["^self$"], "allow-destructuring": true}],
      "valid-typeof": false,
      "typeof-compare": true,
      "arrow-parens": false,
      "object-literal-sort-keys": false,
      "no-parameter-reassignment": true,
      "chai-vague-errors": false,
      "export-name": false,
      "callable-types": false,
      "prefer-array-literal": false,
      "array-type": false,
      "no-implicit-dependencies": false,
      "prefer-method-signature": false,
      "no-typeof-undefined": false,
      "typeof-compare": false,
      "no-reserved-keywords": false,
      "no-suspicious-comment": false,
      "no-single-line-block-comment": false,
      "no-relative-imports": false,
      "no-require-imports": false,
      "import-name": false,
      "no-multiline-string": false,
      "prefer-type-cast": false,
      "missing-jsdoc": false,
      "typedef": false,
      "no-unused-expression": true,
      "no-unused-variable": true,
      "callable-types": true,
      "arrow-parens": [true, "ban-single-arg-parens"],
      "interface-name": false,
      "interface-over-type-literal": true,
      "max-classes-per-file": false,
      "max-line-length": [true, 140],
      "ordered-imports": [true, {"import-sources-order": "any", "named-imports-order": "lowercase-last"}],
      "member-ordering": [true,
        "public-before-private",
        "static-before-instance",
        "variables-before-functions"
      ],
      "no-angle-bracket-type-assertion": true,
      "no-default-export": false,
      "no-empty-interface": true,
      "no-string-throw": true,
      "no-switch-case-fall-through": true,
      "no-unsafe-any": true,
      "prefer-const": true,
      "switch-default": false,
      "variable-name": [true,
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ]
    },
    "rulesDirectory": []
}
